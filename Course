// The purpose of this class is to be able create objects with par.
public class Course extends Golf{
	// Initialize
	int holes;
	int par;
	
	// Constructor
	public Course(int par) {
		this.par = par;
	}
	// Getters
	public int getPar() {
		return par;
	}
}
